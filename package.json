{
  "name": "phaser3-virtual-joystick",
  "version": "1.0.0",
  "description": "A Phaser 3 virtual joystick implementation with phaser3-rex-notes plugin.",
  "keywords": [
    "phaser-webpack",
    "Phaser3-webpack"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "7.5.5",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "expose-loader": "^0.7.5",
    "gulp": "4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "5.0.1",
    "npm-check-updates": "^3.1.20",
    "terser-webpack-plugin": "1.3.0",
    "webpack": "4.37.0",
    "webpack-cli": "3.3.6",
    "webpack-dev-server": "3.7.2"
  },
  "dependencies": {
    "phaser": "^3.18.1"
  },
  "scripts": {
    "update-dependency": "./node_modules/.bin/ncu -u && npm i && npm audit fix",
    "watch": "./node_modules/.bin/concurrently \"npm run webpack:watch\" \"npm run gulp:watch-minify-css\"",
    "build": "./node_modules/.bin/webpack --mode production && npm run gulp:minify-css",
    "webpack:watch": "./node_modules/.bin/webpack --mode development && webpack-dev-server --mode development",
    "gulp:minify-css": "./node_modules/.bin/gulp minify-css",
    "gulp:watch-minify-css": "./node_modules/.bin/gulp watch-minify-css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leandrocurioso/phaser3-virtual-joystick.git"
  },
  "author": {
    "name": "leandrocurioso",
    "email": "leandro.curioso@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/leandrocurioso/phaser3-virtual-joystick/issues"
  },
  "homepage": "https://github.com/leandrocurioso/phaser3-virtual-joystick#readme"
}
